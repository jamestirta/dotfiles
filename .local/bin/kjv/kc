#!/usr/bin/bash
[ -n "$2" ] && [[ "$2" =~ ^[0-9]+$ ]] && echo "$2 $2" > /tmp/kjv/.kjv && exit ||{
[ -n "$2" ] && echo "$2" > /tmp/kjv/.kjv && echo "$2" > /tmp/kjv/.kjv-verse && xdotool key ctrl + l && exit ||{
[ -n "$2" ] && [[ "$2" =~ ^[0-9]+$ ]] && echo "$2" > /tmp/kjv/.kjv-verse && xdotool key ctrl+l && exit;}||{
echo "$2" > /tmp/kjv/.kjv && xdotool key ctrl+l;};}
